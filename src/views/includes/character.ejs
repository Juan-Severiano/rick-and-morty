<ul class="list-characters">
        <% r.forEach((i)=> { %>
        <li class="character d-flex">
            <img src="<%= i.image %>" alt="aaaa">
            <div class="card">
                <h2>
                    <%= i.name %>
                </h2>
                <p class="d-flex align-items-center status">
                    <% if (i.status !=='Alive' ) { %>
                        <i class="bi bi-circle-fill" style="color: red;"></i>
                        <% } else { %>
                            <i class="bi bi-circle-fill" style="color: green;"></i>
                            <% } %>


                                <%= i.status %> - <%= i.species %>
                </p>
                <p class="disable" id="asd">Last known location:</p>
                <p><a href="">
                        <%= i.location.name %>
                    </a></p>
                <p class="disable">First seen:</p>
                <p><a href=""><%= i.firstEpisodeName %></a></p>
            </div>
        </li>
        <% }) %>
    </ul>